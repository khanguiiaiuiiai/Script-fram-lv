local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()

-- ========================================
-- AUTO NH·∫¨P T·∫§T C·∫¢ CODE GAME
-- ========================================
local function autoRedeemCodes()
    local HttpService = game:GetService("HttpService")
    local codes = {}

    -- 1. Th·ª≠ l·∫•y danh s√°ch code t·ª´ pastebin (c·∫≠p nh·∫≠t th∆∞·ªùng xuy√™n)
    --    B·∫°n c√≥ th·ªÉ thay link raw b·∫±ng link code m·ªõi nh·∫•t t·ª´ c·ªông ƒë·ªìng
    local success, result = pcall(function()
        return HttpService:GetAsync("https://pastebin.com/raw/your_raw_link_here")  -- ƒê·ªïi link n√†y
    end)
    if success then
        -- Gi·∫£ s·ª≠ pastebin tr·∫£ v·ªÅ JSON array ["code1","code2",...]
        codes = HttpService:JSONDecode(result)
    else
        -- N·∫øu kh√¥ng l·∫•y ƒë∆∞·ª£c, d√πng danh s√°ch code c≈© (v·∫´n ho·∫°t ƒë·ªông ƒë∆∞·ª£c v√†i code)
        codes = {
            "Sub2CaptainMaui", "Sub2NoobMaster123", "Sub2Daigrock",
            "Axiore", "TantaiGaming", "MagicBus", "JCWK", "Starcodeheo",
            "KittGaming", "Fudd10", "Fudd10_V2", "Bluxxy", "THEGREATACE",
            "UPD15", "KITT_RESET", "Sub2UncleKizaru", "Axiore", "TantaiGaming"
        }
    end

    -- 2. T√¨m remote ƒë·ªÉ g·ª≠i l·ªánh nh·∫≠p code
    local ReplicatedStorage = game:GetService("ReplicatedStorage")
    local remotes = ReplicatedStorage:FindFirstChild("Remotes")
    local commF = remotes and remotes:FindFirstChild("CommF_")
    if not commF then
        warn("Kh√¥ng t√¨m th·∫•y remote CommF_! Code s·∫Ω kh√¥ng ƒë∆∞·ª£c nh·∫≠p.")
        return
    end

    -- 3. Duy·ªát t·ª´ng code v√† g·ª≠i l·ªánh
    for _, code in ipairs(codes) do
        local args = { [1] = "RedeemCode", [2] = code }
        local success, msg = pcall(function()
            commF:InvokeServer(unpack(args))
        end)
        if success then
            print("‚úÖ ƒê√£ nh·∫≠p code: " .. code)
        else
            print("‚ùå Code kh√¥ng h·ª£p l·ªá ho·∫∑c ƒë√£ h·∫øt h·∫°n: " .. code)
        end
        task.wait(1)  -- Ngh·ªâ 1s gi·ªØa c√°c l·∫ßn nh·∫≠p ƒë·ªÉ tr√°nh spam
    end
end

-- ========================================
-- AUTO FARM LEVEL (CH·ªà FARM LV)
-- ========================================
local function autoFarm()
    while true do
        task.wait(0.5)

        -- ƒê·∫£m b·∫£o nh√¢n v·∫≠t t·ªìn t·∫°i
        character = player.Character
        if not character or not character:FindFirstChild("HumanoidRootPart") then
            character = player.CharacterAdded:Wait()
        end

        -- T√¨m qu√°i g·∫ßn nh·∫•t trong folder Enemies
        local nearestEnemy = nil
        local shortestDistance = math.huge
        local enemiesFolder = workspace:FindFirstChild("Enemies")
        if not enemiesFolder then
            -- N·∫øu kh√¥ng c√≥ folder Enemies (c√≥ th·ªÉ ƒëang ·ªü ƒë·∫£o kh√°c), t√¨m t·∫•t c·∫£ enemy trong workspace
            -- (ƒë∆°n gi·∫£n ho√°, nh∆∞ng t·ªët nh·∫•t n√™n d√πng folder Enemies)
            goto continue
        end

        for _, enemy in pairs(enemiesFolder:GetChildren()) do
            local humanoid = enemy:FindFirstChild("Humanoid")
            if humanoid and humanoid.Health > 0 then
                local root = enemy:FindFirstChild("HumanoidRootPart")
                if root then
                    local distance = (root.Position - character.HumanoidRootPart.Position).Magnitude
                    if distance < shortestDistance then
                        shortestDistance = distance
                        nearestEnemy = enemy
                    end
                end
            end
        end

        if nearestEnemy then
            -- Trang b·ªã v≈© kh√≠ n·∫øu ch∆∞a c√≥
            local tool = character:FindFirstChildOfClass("Tool")
            if not tool then
                tool = player.Backpack:FindFirstChildWhichIsA("Tool")
                if tool then
                    tool.Parent = character
                end
            end

            -- Di chuy·ªÉn ƒë·∫øn qu√°i n·∫øu xa
            if shortestDistance > 5 then
                character.Humanoid:MoveTo(nearestEnemy.HumanoidRootPart.Position)
            else
                -- T·∫•n c√¥ng b·∫±ng tool hi·ªán t·∫°i
                tool = character:FindFirstChildOfClass("Tool")
                if tool then
                    tool:Activate()
                else
                    -- N·∫øu kh√¥ng c√≥ tool, th·ª≠ d√πng remote combat (c∆° b·∫£n)
                    local remotes = game:GetService("ReplicatedStorage"):FindFirstChild("Remotes")
                    if remotes then
                        local combat = remotes:FindFirstChild("Combat")
                        if combat then
                            combat:FireServer(nearestEnemy.HumanoidRootPart.CFrame)
                        end
                    end
                end
            end
        end
        ::continue::
    end
end

-- ========================================
-- K√çCH HO·∫†T C·∫¢ HAI T√ÅC V·ª§
-- ========================================
task.spawn(autoRedeemCodes)
task.spawn(autoFarm)

print("üöÄ Script ƒë√£ ch·∫°y! Auto farm level v√† auto nh·∫≠p code ƒëang ho·∫°t ƒë·ªông.")
